{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "paths": {
      "@bindings/*": [
        "../backend/bindings/*"
      ],
      "@/*": [
        "./src/*"
      ],
      "$lib": [
        "./src/lib"
      ],
      "$lib/*": [
        "./src/lib/*"
      ]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}